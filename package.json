    "Krivonos Ivan <devbackend@yandex.ru>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devbackend/ngx-onscreen-keyboard/issues"
  },
  "homepage": "https://github.com/devbackend/ngx-onscreen-keyboard#readme",
  "peerDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "^4.0.0",
